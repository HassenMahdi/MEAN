var DynamicSurveyComponent = (function () {
    function DynamicSurveyComponent() {
        this.questions = [];
    }
    DynamicSurveyComponent.prototype.ngOnInit = function () {
    };
    DynamicSurveyComponent.prototype.deleteSurveyQuestion = function (e, i) {
        e.preventDefault();
        this.questions.splice(i, 1);
    };
    DynamicSurveyComponent.prototype.addSurveyQuestion = function (e, type) {
        e.preventDefault();
        switch (type) {
            case "qa":
                this.questions.push({
                    content: "",
                    type: "qa",
                    typeFull: "Question/Answer"
                });
                console.log("type1");
                break;
            case "sr":
                this.questions.push({
                    content: "",
                    type: "sr",
                    typeFull: "Star Rating"
                });
                break;
            case "po":
                this.questions.push({
                    content: "",
                    type: "po",
                    typeFull: "Poll",
                    bchoice: true,
                    choices: ["choice 1", "choice 2"]
                });
                break;
            case "mc":
                this.questions.push({
                    content: "",
                    type: "mc",
                    typeFull: "Multiple choise",
                    bchoice: true,
                    choices: ["choice 1", "choice 2"]
                });
                break;
            default: console.log("Unknowen type");
        }
    };
    __decorate([
        __webpack_require__.i(__WEBPACK_IMPORTED_MODULE_0__angular_core__["Input"])(), 
