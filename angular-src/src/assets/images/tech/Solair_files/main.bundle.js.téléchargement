        this.msg1 = null;
        this.msg2 = null;
        this.msg3 = null;
        if (this.newpass == "") {
            this.msg2 = "Please enter a new password";
        }
        else if (this.newpassc != this.newpass) {
            this.msg3 = "Different passwords have been entered";
        }
        if (this.oldpass == "") {
            this.msg1 = "Please enter your old password";
        }
        else if (this.newpass != "" && this.newpass == this.oldpass) {
            this.msg2 = "New password cannot be the same as the old one";
        }
        if (this.msg1 || this.msg2 || this.msg3)
            return;
        this.authService.changePassword(this.user._id, this.oldpass, this.newpass).subscribe(function (res) {
            if (res.success) {
                _this.toastr.success("Your password have been changed");
                _this.oldpass = "";
                _this.newpass = "";
                _this.newpassc = "";
                $('#closechangepass').click();
                $('body').click();
            }
            else if (!res.success) {
                _this.msg1 = "Wrong password";
            }
        });
    };
    ProfileComponent.prototype.loadImage = function () {
        this.image.loaded = true;
    };
    ProfileComponent.prototype.urlChange = function (e) {
        this.image = {
            name: "N/A",
            url: e.target.value,
            size: "N/A",
            source: "url",
            loaded: false,
        };
    };
    ProfileComponent.prototype.saveImage = function () {
        var _this = this;
