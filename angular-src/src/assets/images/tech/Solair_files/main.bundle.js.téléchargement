var SurveysService = (function () {
    function SurveysService(http) {
        this.http = http;
    }
    SurveysService.prototype.getSurveys = function () {
        console.log('http://localhost:3000/surveys/get/');
        return this.http.get('http://localhost:3000/surveys/get/')
            .map(function (res) { return res.json(); });
    };
    SurveysService.prototype.getOwnedSurveys = function (owner_id) {
        return this.http.get('http://localhost:3000/surveys/get/owner/' + owner_id)
            .map(function (res) { return res.json(); });
    };
    SurveysService.prototype.getTeamsSurvey = function (team_id_list) {
        return this.http.get('http://localhost:3000/surveys/get/teams/' + team_id_list.toString())
            .map(function (res) { return res.json(); });
    };
    SurveysService.prototype.CreateSurvey = function (survey) {
        var headers = new __WEBPACK_IMPORTED_MODULE_1__angular_http__["Headers"]();
        headers.append('Content-Type', 'application/json');
        return this.http.post('http://localhost:3000/surveys/add', survey)
            .map(function (res) { return res.json(); });
    };
    SurveysService.prototype.deleteSurvey = function (survey_id) {
        return this.http.delete('http://localhost:3000/surveys/' + survey_id)
            .map(function (res) { return res.json(); });
    };
    SurveysService.prototype.submitSurveyAnswers = function (survey_id, submitter_id, answers) {
        var body = {
            sub: {
                submitter_id: submitter_id,
                answers: answers,
            },
            survey_id: survey_id,
        };
        var headers = new __WEBPACK_IMPORTED_MODULE_1__angular_http__["Headers"]();
        headers.append('Content-Type', 'application/json');
        return this.http.post('http://localhost:3000/surveys/add/answers', body)
            .map(function (res) { return res.json(); });
    };
    SurveysService.prototype.formatSurveys = function (surveys) {
        if (surveys == null)
            return;
        surveys.forEach(function (element) {
            if (new Date(element.enddate) >= new Date()) {
                element.active = true;
                element.activeclass = "activesurvey";
            }
            else {
                element.active = "false";
                element.activeclass = "inactivesurvey";
            }
            element.begindate = dateFormat(element.begindate, "dd/mm/yyyy");
            element.enddate = dateFormat(element.enddate, "dd/mm/yyyy");
        });
    };
    SurveysService.prototype.filterSurveysByStatus = function (surveys, getActive) {
        console.log(surveys);
        if (surveys == null)
            return;
        surveys = surveys.filter(function (element, index) {
            if (new Date(element.enddate) >= new Date()) {
                element.active = true;
            }
            else {
                element.active = false;
            }
            if (getActive)
                return element.active;
            else
                return !element.active;
        });
        return surveys;
    };
    SurveysService.prototype.formatAnswersTable = function (questions, subs) {
        var table = new Array(subs.length);
        for (var i = 0; i < subs.length; i++) {
            table[i] = new Array(questions.length);
        }
        var qList = [];
        try {
            questions.pages.forEach(function (page) {
                page.elements.forEach(function (element) {
                    qList.push(element.name);
                });
            });
            qList.forEach(function (question, col) {
                subs.forEach(function (sub, row) {
                    var an = sub.answers;
                    var answers = [];
                    for (var key in an) {
                        for (var key2 in an[key]) {
                            answers.push({
                                q: key2,
                                a: an[key][key2]
                            });
                        }
                    }
                    for (var i = 0; i < answers.length; i++) {
                        var answer = answers[i];
                        if (answer.q == question) {
                            table[row][col] = answer.a;
                            sub.answers.splice(i, 1);
                            break;
                        }
                    }
                });
            });
        }
        catch (e) {
            console.log("Bad survey format");
        }
        return {
            answers: table,
            questions: qList,
        };
    };
    SurveysService = __decorate([
        __webpack_require__.i(__WEBPACK_IMPORTED_MODULE_0__angular_core__["Injectable"])(), 
        __metadata('design:paramtypes', [(typeof (_a = typeof __WEBPACK_IMPORTED_MODULE_1__angular_http__["Http"] !== 'undefined' && __WEBPACK_IMPORTED_MODULE_1__angular_http__["Http"]) === 'function' && _a) || Object])
    ], SurveysService);
    return SurveysService;
    var _a;
}());
