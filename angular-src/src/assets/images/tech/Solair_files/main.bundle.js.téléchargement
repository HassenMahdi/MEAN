var CreateSurveyComponent = (function () {
    function CreateSurveyComponent(authService, toasrt, surveysService) {
        this.authService = authService;
        this.toasrt = toasrt;
        this.surveysService = surveysService;
        this.invalideMsg = [];
        this.componentData = [];
        this.target_team_id = "";
        this.beginDatePicker = {
            // other options...
            dateFormat: 'dd.mm.yyyy',
        };
        this.endDatePicker = {
            // other options...
            dateFormat: 'dd.mm.yyyy',
        };
        this.currentDate = new Date();
        this.tommorowDate = new Date(this.currentDate.getTime() + 24 * 60 * 60 * 1000);
        this.endDate = {
            date: {
                day: this.tommorowDate.getDate(),
                month: this.tommorowDate.getMonth(),
                year: this.tommorowDate.getFullYear()
            }
        };
        this.beginDate = {
            date: {
                day: this.currentDate.getDate(),
                month: this.currentDate.getMonth(),
                year: this.currentDate.getFullYear()
            }
        };
    }
    CreateSurveyComponent.prototype.ngOnInit = function () {
        var _this = this;
        this.authService.getProfile().subscribe(function (profile) {
            _this.user = profile.user;
            $(".container-fluid").removeClass("fadeIn animated");
        }, function (err) {
            console.log(err);
            return false;
        });
    };
    CreateSurveyComponent.prototype.deleteSurveyQuestion = function (e, index) {
        e.preventDefault();
        this.questions.splice(index, 1);
    };
    CreateSurveyComponent.prototype.submitSurvey = function () {
        var _this = this;
        this.invalideMsg = [];
        if (this.name == null)
            this.invalideMsg.push("Insert a valid name.");
        if (this.validDates(this.beginDate, this.endDate))
            this.invalideMsg.push("Please enter valid dates.");
        if (this.target_team_id == "")
            this.invalideMsg.push("Select a team.");
        if (this.questions == null)
            this.invalideMsg.push("Save your changes before submitting.");
        if (this.questions == null || this.questions == [])
            this.invalideMsg.push("The survey is empty");
        if (this.invalideMsg.length <= 0) {
            var survey = {
                team_id: this.target_team_id,
                owner_id: this.user._id,
                name: this.name,
                subject: this.desc,
                begindate: new Date(this.beginDate.date.year, this.beginDate.date.month, this.beginDate.date.day),
                enddate: new Date(this.endDate.date.year, this.endDate.date.month, this.endDate.date.day),
                questions: this.questions
            };
            this.surveysService.CreateSurvey(survey).subscribe(function (res) {
                if (res.success)
                    _this.toasrt.success("Your team members will be notified", "Great!");
                else
                    _this.toasrt.error("Something went wrong.", "Oops!");
            }, function (err) {
                _this.toasrt.error("Something went wrong.", "Oops!");
            });
        }
    };
    CreateSurveyComponent.prototype.validDates = function (beginDate, endDate) {
        var bd = new Date(beginDate.date.year, beginDate.date.month, beginDate.date.day);
        var ed = new Date(endDate.date.year, endDate.date.month, endDate.date.day);
        if (ed > bd)
            return false;
        else
            return true;
    };
    CreateSurveyComponent.prototype.onSurveySaved = function (survey) {
        this.questions = survey;
    };
    CreateSurveyComponent = __decorate([
        __webpack_require__.i(__WEBPACK_IMPORTED_MODULE_0__angular_core__["Component"])({
