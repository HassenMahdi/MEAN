var FileService = (function () {
    function FileService(http) {
        this.http = http;
        this.url = "http://localhost:3000";
        this.filesizelimit = 4000000;
    }
    FileService.prototype.uploadImage = function (user_id, file) {
        var _this = this;
        var headers = new __WEBPACK_IMPORTED_MODULE_2__angular_http__["Headers"]();
        var formData = new FormData();
        formData.append('image', file, file.name);
        formData.append('user_id', user_id);
        if (file.size < this.filesizelimit)
            return this.http.post(this.url + "/files/image", formData, { headers: headers })
                .map(function (res) { return res.json(); });
        else {
            var ob = new __WEBPACK_IMPORTED_MODULE_4__angular_core_src_facade_async__["a" /* Observable */](function (observer) { return observer.next({ success: false, msg: "File too large (Limit: " + _this.filesizelimit / 1000000 + " Mb)" }); });
            return ob;
        }
    };
    FileService.prototype.uploadFile = function (user_id, file) {
        var _this = this;
        var headers = new __WEBPACK_IMPORTED_MODULE_2__angular_http__["Headers"]();
        var formData = new FormData();
        formData.append('file', file, file.name);
        formData.append('user_id', user_id);
        if (file.size < this.filesizelimit)
            return this.http.post(this.url + "/files", formData, { headers: headers })
                .map(function (res) { return res.json(); });
        else {
            var ob = new __WEBPACK_IMPORTED_MODULE_4__angular_core_src_facade_async__["a" /* Observable */](function (observer) { return observer.next({ success: false, msg: "File too large (Limit: " + _this.filesizelimit / 1000000 + " Mb)" }); });
            return ob;
        }
    };
    FileService.prototype.downloadFile = function (filename) {
        var headers = new __WEBPACK_IMPORTED_MODULE_2__angular_http__["Headers"]();
        return this.http.get(this.url + "/files/" + filename, { headers: headers, responseType: __WEBPACK_IMPORTED_MODULE_2__angular_http__["ResponseContentType"].Blob })
            .map(function (res) { return res.blob(); })
            .subscribe(function (res) {
            __webpack_require__.i(__WEBPACK_IMPORTED_MODULE_1_file_saver__["saveAs"])(res, filename);
            //var url= window.URL.createObjectURL(res);
            //window.open(url);
